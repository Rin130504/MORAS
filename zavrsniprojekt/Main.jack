class Main {
    function void main() {
        var RockPaperScissors game;
        var int userChoice;
        
let game = RockPaperScissors.new();
        
        do Output.printString("Enter 0 for Rock, 1 for Paper, or 2 for Scissors: ");
        let userChoice = Keyboard.readInt();
        
do game.play(userChoice);
        
        return;
    }
}